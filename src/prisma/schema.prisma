generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" // O caminho do arquivo do banco de dados SQLite
}

model legend {
  id        String   @id @default(cuid()) // Gerar automaticamente um ID Ãºnico
  name      String
  Strength  Int
  Dexterity Int
  Defense   Int
  Speed     Int
  Weapon_one   String
  Weapon_two   String
}
